{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.trailingComma": "es5",
  "prettier.requireConfig": false,
  "prettier.singleQuote": true,
  "prettier.useTabs": true,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 100,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": [
    "source.formatDocument",
    "source.fixAll.eslint"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ff8952",
    "activityBar.background": "#ff8952",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#bfffd4",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#ff8952",
    "statusBar.background": "#ff661f",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#eb4a00",
    "statusBarItem.remoteBackground": "#ff661f",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#ff661f",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#ff661f99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#FF661F",
}
